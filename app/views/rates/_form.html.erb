<%= form_for(rate) do |f| %>
  <% if rate.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(rate.errors.count, "error") %> prohibited this rate from being saved:</h2>

      <ul>
      <% rate.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :origin_id %>
    <!-- <%= f.text_field :origin_id %> -->
    <%= f.collection_select :origin_id, Port.all, :id, :port_code  %>
  </div>

  <div class="field">
    <%= f.label :destination_id %>
    <!-- <%= f.text_field :destination_id %> -->
    <%= f.collection_select :destination_id, Port.all, :id, :port_code  %>

  </div>

  <div class="field">
    <%= f.label :carrier_id %>
    <!-- <%= f.text_field :carrier_id %> -->
    <%= f.collection_select :carrier_id, Carrier.all, :id, :name %>

  </div>

  <div class="field">
    <%= f.label :shipment_type_id %>
    <!-- <%= f.text_field :shipment_type_id %> -->
    <%= f.collection_select :shipment_type_id, ShipmentType.all, :id, :name %>

  </div>

  <div class="field">
    <%= f.label :cost %>
    <%= f.text_field :cost %>
  </div>

  <div class="field">
    <%= f.label :unit_of_measure_id %>
    <!-- <%= f.text_field :unit_of_measure_id %> -->
    <%= f.collection_select :unit_of_measure_id, UnitOfMeasure.all, :id, :name %>

  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
