<p id="notice"><%= notice %></p>

<h1>Shipment Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipment_types.each do |shipment_type| %>
      <tr>
        <td><%= shipment_type.name %></td>
        <td><%= link_to 'Show', shipment_type %></td>
        <td><%= link_to 'Edit', edit_shipment_type_path(shipment_type) %></td>
        <td><%= link_to 'Destroy', shipment_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipment Type', new_shipment_type_path %>
