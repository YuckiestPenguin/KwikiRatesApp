<p id="notice"><%= notice %></p>

<h1>Port Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @port_types.each do |port_type| %>
      <tr>
        <td><%= port_type.name %></td>
        <td><%= link_to 'Show', port_type %></td>
        <td><%= link_to 'Edit', edit_port_type_path(port_type) %></td>
        <td><%= link_to 'Destroy', port_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Port Type', new_port_type_path %>
